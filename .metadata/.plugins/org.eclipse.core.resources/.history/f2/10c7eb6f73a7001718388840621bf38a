import java.util.ArrayList;

public class Node {

	private State state;
	private ArrayList<Node> nodes = new ArrayList<Node>();
	
	Node(State state, ArrayList<Node> nodes) {
		
		this.state = state;
		this.nodes = nodes;
	}

	public State getState() {
		return state;
	}

	public ArrayList<Node> getNodes() {
		return nodes;
	}
	
	public void addNode(Node node) {
		nodes.add(node);
	}
}